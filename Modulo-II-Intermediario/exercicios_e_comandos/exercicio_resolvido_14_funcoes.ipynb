   "source": [
    "def Divisivel(A, B):\n",
    "    return True if A % B == 0 else False\n",
    "\n",
    "valorA = int(input('Digite A: '))\n",
    "valorB = int(input('Digite B: '))\n",
    "if Divisivel(valorA, valorB):\n",
    "    print(f'{valorA} é divisível por {valorB}')\n",
    "else:\n",
    "    print(f'{valorA} não é divisível por {valorB}')\n"
   ]
  }
